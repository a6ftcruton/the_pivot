<h1>Edit Food Category</h1>
<%= render 'form' %>

<div class='btn-toolbar' role='toolbar'>

  <div class='btn-group'>
    <%= button_to "Delete this Category", admin_category_path(@category), method: :delete, data: { confirm: 'Are you sure you wish to delete?' } %>
  </div>

  <div class='btn-group'>
    <%= button_to "Cancel", admin_categories_path, method: 'get' %>
  </div>
</div>
